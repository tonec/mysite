/*
 * ==========================================================
 * Menu styles
 * ==========================================================
 */

.menu-toggle { display:none; }
.header-section-wrapper {
  background-color: #333;
}
.header-section-wrapper .header-second-zone { float:right; }
.ssrg-header-logo {
  position:absolute;
  z-index:11;
  top:0px;
}
.ssrg-header-logo a.header_logo {
  background:url(/assets/layouts/SR14/images/header-and-footer-sprite.png) no-repeat 0 0 transparent;
  height:131px;
  width:98px;
  display:block;
  text-decoration: none;
}
.header--sainsburys-logo {
  height: 51px;
  width: 138px;
  float: right;
  margin-left: 20px;
  display: block;
}
.header--sainsburys-logo a {
  height: 51px;
  width: 138px;
  display: block;
  background:url(/assets/layouts/SR14/images/header-and-footer-sprite.png) no-repeat scroll -342px -237px transparent;
  text-decoration: none;
}
.header--sainsburys-logo a:hover {
  text-decoration: none;
}
.zone-header-wrapper { height:66px; }
.zone-menu-wrapper {
  height:44px;
  display:block;
}
.zone-menu-wrapper .region-menu div#block-main-menu h2.block-title, .header-section-wrapper .zone-menu-wrapper .region-menu nav.navigation h2.block-title { display:none; }
.zone-menu-wrapper .region-menu .menu {
  padding:0;
  margin:0;
  background-color:transparent;
  height:44px;
}
.zone-menu-wrapper .region-menu .menu .menu { display:none; }
.zone-menu-wrapper .region-menu .menu li {
  height:100%;
  padding:0;
  margin:0;
  background:url(data:image/jpeg;base64,) no-repeat right center transparent;
  border:none;
  position:relative;
  display:block;
  float:left;
  cursor:pointer;
  outline:none;
}
.zone-menu-wrapper .region-menu .menu li a {
  color:white;
  font-weight:bold;
  text-align:center;
  text-transform:uppercase;
  font-size:16px;
  line-height:44px;
  height:100%;
  text-align:center;
  display:block;
  -moz-box-sizing:border-box;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
  font-family:'foco', arial;
  outline:none;
  text-decoration: none;
}
.zone-menu-wrapper .region-menu .menu li a:hover {
  background-color: white;
  padding-left: 2px;
  text-decoration: none;
  color: #333;
  -webkit-border-radius: 3px 3px 0 0;
  -ms-border-radius: 3px 3px 0 0;
  -moz-border-radius: 3px 3px 0 0;
  border-radius: 3px 3px 0 0;
}
.zone-menu-wrapper .region-menu .menu li:hover { margin-left:-2px; }
.zone-menu-wrapper .region-menu .menu li.first {
  background:none no-repeat 0 0 transparent;
  text-indent:-999em;
}
.zone-menu-wrapper .region-menu .menu li.first a:hover {
  background-color:transparent;
  -webkit-border-radius: 0;
  -ms-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  padding-left:0px;
}
.zone-menu-wrapper .region-menu .menu li.last:hover { margin-left:-2px; }
.zone-menu-wrapper .region-menu .menu li.has-dropdown { margin-left:-2px; }
.zone-menu-wrapper .region-menu .menu li.has-dropdown a {
  background-color:#2a2a2a;
  padding-left:2px;
}
.zone-menu-wrapper .region-menu .menu li.has-dropdown a:after {
  background:url(data:image/png;base64,) no-repeat center bottom transparent;
  content:"";
  position:absolute;
  bottom:0;
  left:50%;
  margin-left:-23px;
  height:15px;
  width:47px;
  padding:0;
}
.zone-menu-wrapper .region-menu .menu li.menu-home { width:115px; }
.zone-menu-wrapper .region-menu .menu li.menu-enter-now { width:141px; }
.zone-menu-wrapper .region-menu .menu li.menu-event-info { width:139px; }
.zone-menu-wrapper .region-menu .menu li.menu-schools { width:122px; }
.zone-menu-wrapper .region-menu .menu li.menu-fundraise { width:135px; }
.zone-menu-wrapper .region-menu .menu li.menu-order-stuff { width:152px; }
.zone-menu-wrapper .region-menu .menu li.menu-what-we-do { width:150px; }
.zone-menu-wrapper .region-menu .menu li.menu-support-sport-relief { width:226px; }
.zone-menu-wrapper .region-menu .menu li.menu-home:hover { width:117px; }
.zone-menu-wrapper .region-menu .menu li.menu-enter-now:hover { width:143px; }
.zone-menu-wrapper .region-menu .menu li.menu-event-info.has-dropdown, .header-section-wrapper .zone-menu-wrapper .region-menu .menu li.menu-event-info:hover { width:141px; }
.zone-menu-wrapper .region-menu .menu li.menu-schools.has-dropdown, .header-section-wrapper .zone-menu-wrapper .region-menu .menu li.menu-schools:hover { width:124px; }
.zone-menu-wrapper .region-menu .menu li.menu-fundraise.has-dropdown, .header-section-wrapper .zone-menu-wrapper .region-menu .menu li.menu-fundraise:hover { width:137px; }
.zone-menu-wrapper .region-menu .menu li.menu-order-stuff.has-dropdown, .header-section-wrapper .zone-menu-wrapper .region-menu .menu li.menu-order-stuff:hover { width:154px; }
.zone-menu-wrapper .region-menu .menu li.menu-what-we-do:hover { width:152px; }
.zone-menu-wrapper .region-menu .menu li.menu-support-sport-relief.has-dropdown, .header-section-wrapper .zone-menu-wrapper .region-menu .menu li.menu-support-sport-relief:hover { width:228px; }


// Giving Page tabs - these use primary nav styling

.zone-menu-wrapper .region-menu .menu li.my-page { width:108px; }
.zone-menu-wrapper .region-menu .menu li.my-team { width:108px; }
.zone-menu-wrapper .region-menu .menu li.get-sponsors { width:148px; }
.zone-menu-wrapper .region-menu .menu li.team-options { width:144px; }
.zone-menu-wrapper .region-menu .menu li.top-tips { width:104px; }
.zone-menu-wrapper .region-menu .menu li.page-options { width:147px; }

.zone-menu-wrapper .region-menu .menu li.my-page:hover { width:110px; }
.zone-menu-wrapper .region-menu .menu li.my-team:hover { width:110px; }
.zone-menu-wrapper .region-menu .menu li.get-sponsors:hover { width:150px; }
.zone-menu-wrapper .region-menu .menu li.team-options:hover { width:146px; }
.zone-menu-wrapper .region-menu .menu li.top-tips:hover { width:106px; }
.zone-menu-wrapper .region-menu .menu li.page-options:hover { width:149px; }

.zone-menu-wrapper .region-menu .menu li.log-out {
  position: absolute;
  top: 8px;
  right: 160px;
  background: none;
  font-size: 16px;
}
.zone-menu-wrapper .region-menu .menu li.log-out a {
  text-transform: none;
  font-weight: normal;
  font-size: 16px;
  color: #ddd;
}
.zone-menu-wrapper .region-menu .menu li.log-out a:hover { background-color: transparent; color: #ddd; }
.zone-menu-wrapper .region-menu .menu li.log-out a span {
  background: url(/assets/layouts/SR14/images/ui/sprite.png) no-repeat -428px -45px;
  width: 20px;
  height: 20px;
  display: inline-block;
  position: relative;
  margin: 0 0 -3px 10px;
}

@media all and (min-width: 980px) and (max-width: 1250px) {

  .header-section-wrapper .zone-menu-wrapper .region-menu .menu li a { font-size:1.1em; }
  .header-section-wrapper .zone-menu-wrapper .region-menu .menu li a:hover { padding-left:2px; }
