// Icon font
@font-face {
	font-family: 'sr-icons';
	src:url('/assets/layouts/SR14/fonts/sr-icons.eot');
	src:url('/assets/layouts/SR14/fonts/sr-icons.eot?#iefix') format('embedded-opentype'),
		url('/assets/layouts/SR14/fonts/sr-icons.woff') format('woff'),
		url('/assets/layouts/SR14/fonts/sr-icons.ttf') format('truetype'),
		url('/assets/layouts/SR14/fonts/sr-icons.svg#sr-icons') format('svg');
	font-weight: normal;
	font-style: normal;
}

.sr-icon {
	font-family: 'sr-icons';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	font-size: 22px;
	color: #848484;
	line-height: 1;
	display: inline-block;
	width: 22px;
	height:22px;
	vertical-align: text-bottom;
	-webkit-font-smoothing: antialiased;
}

a:hover .sr-icon{ color: $red; }

